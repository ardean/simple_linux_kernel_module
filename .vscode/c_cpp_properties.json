{
  "configurations": [
    {
      "name": "Linux Kernel Module Development",
      "browse": {
        "path": [
          "${workspaceFolder}/**",
          "/usr/src/linux-headers-5.15.0-57-generic/include/",
          "/usr/src/linux-headers-5.15.0-57-generic/arch/x86/include/",
          "/usr/src/linux-headers-5.15.0-57-generic/arch/x86/include/generated/"
        ],
        "limitSymbolsToIncludedHeaders": false
      },
      "includePath": [
        "${workspaceFolder}/**",
        "/usr/src/linux-headers-5.15.0-57-generic/include/",
        "/usr/src/linux-headers-5.15.0-57-generic/arch/x86/include/",
        "/usr/src/linux-headers-5.15.0-57-generic/arch/x86/include/generated/"
      ],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "linux-gcc-x64",
      "compilerArgs": [
        ""
      ],
      "defines": [
        "__GNUC__",
        "__KERNEL__",
        "MODULE"
      ]
    }
  ],
  "version": 4
}